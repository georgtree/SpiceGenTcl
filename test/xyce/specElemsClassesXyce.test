testTemplate testResistorClass-9 {} {Resistor new 1 netp netm -tc1 1 -r V(a)+V(b)+pow(V(c),2) -beh}\
        {r1 netp netm r={V(a)+V(b)+pow(V(c),2)} tc1=1}

